{
  "name": "st-nucleo-f446re-gcc",
  "version": "0.1.0",
  "description": "mbed build target for the ST Nucleo f446re development board.",
  "author": "swkim01@gmail.com",
  "repository": {
    "url": "git@github.com:swkim01/st-nucleo-f446re-gcc.git",
    "type": "git"
  },
  "homepage": "https://github.com/swkim01/st-nucleo-f446re-gcc",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "mbed-gcc": "*"
  },
  "keywords": [
    "mbed-target:nucleo-f446re",
    "nucleo-f446re",
    "nucleo",
    "x-nucleo",
    "ST",
    "STM",
    "gcc"
  ],
  "similarTo": [
    "nucleo-f446re",
    "f446re",
    "f446",
    "nucleo",
    "stm32f446ret6",
    "stm32f446re",
    "stm32f446xx",
    "stm32f446",
    "stm32f4",
    "stm32",
    "st",
    "cortex-m4",
    "armv7-m",
    "arm",
    "gcc",
    "mbed",
    "stm32f446re-no-inherit",
    "stm32f446xx-no-inherit"
  ],
  "config": {
    "mbed-os": {},
    "cmsis": {
      "nvic": {
