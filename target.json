        "flash_vector_address": "0x08000000",
        "user_irq_offset": 16,
        "user_irq_number": 85
      }
    },
    "uvisor": {
      "present": 0
    },
    "hardware": {
      "externalClock": "8000000",
      "internalClock": "16000000",
      "pins": {
        "LED1": "PA_5",
        "LED2": "PA_5",
        "LED3": "PG_5",
        "LED4": "PG_5",
        "USER_BUTTON": "PC_13",
        "BUTTON1": "PC_13",
        "SERIAL_TX": "PA_2",
        "SERIAL_RX": "PA_3",
        "USBTX": "PA_2",
        "USBRX": "PA_3",
        "I2C_SCL": "PB_8",
        "I2C_SDA": "PB_9",
        "SPI_MOSI": "PA_7",
        "SPI_MISO": "PA_6",
        "SPI_SCK": "PA_5",
        "SPI_CS": "PA_6",
        "PWM_OUT": "PB_3",
        "A0": "PA_0",
        "A1": "PA_1",
        "A2": "PA_4",
        "A3": "PB_0",
        "A4": "PC_1",
        "A5": "PC_0",
        "D0": "PA_3",
        "D1": "PA_2",
        "D2": "PA_10",
        "D3": "PB_3",
        "D4": "PB_5",
        "D5": "PB_4",
        "D6": "PB_10",
        "D7": "PA_8",
        "D8": "PA_9",
        "D9": "PC_7",
        "D10": "PB_6",
        "D11": "PA_7",
        "D12": "PA_6",
        "D13": "PA_5",
        "D14": "PB_9",
        "D15": "PB_8"
      },
      "test-pins": {
        "spi": {
          "mosi": "SPI_MOSI",
          "miso": "SPI_MISO",
          "sclk": "SPI_SCK",
          "ssel": "SPI_CS"
        },
        "i2c": {
          "sda": "I2C_SDA",
          "scl": "I2C_SCL"
        },
        "serial": {
          "tx": "PA_9",
          "rx": "PC_7"
        }
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug": [
    "valinor",
    "--target",
    "stm32f446re",
    "$program"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "--target",
      "ST_NUCLEO_F446RE",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "ST_NUCLEO_F446RE",
      "$program"
    ]
  }
}
